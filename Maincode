Rayfield:LoadConfiguration()

-- Cleanup
Rayfield:DestroySignal():Connect(function()
    for _, connection in ipairs(silentAimConnections) do
        connection:Disconnect()
    end
    if fovCircle then
        fovCircle:Remove()
        fovCircle = nil
    end
end)
